local function custom_kick(title, desc, button)
game.Players.LocalPlayer:kick()
for _, v in pairs(game.CoreGui.RobloxPromptGui.promptOverlay:GetDescendants()) do
    if v.Name == "ErrorTitle" then -- title
        v.Text = title
    elseif v.Name == "ErrorMessage" then -- description
        v.Text = desc
    elseif v.Name == "ButtonText" then -- leave button
        v.Text = button
    end
end
end

custom_kick("Message", "Wait Update!!!\n(Error Code: -1)", "Ok!!!")
